#include<iostream>
using namespace std;
int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		unsigned long long int n;
		cin>>n;
		unsigned long long int a[n];
		unsigned long long int s=0;
		for(int i=0;i<n;i++)
		{
		
		 
		cin>>a[i];
		
		s=(s%n+a[i]%n)%n;
		
		}
		if(s%n==0)
		cout<<"YES"<<endl;
		else
		cout<<"NO"<<endl;
	}
	
	return 0;
}