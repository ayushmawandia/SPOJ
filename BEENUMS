#include<iostream>

#include<cmath>
using namespace std;
int main()
{
	long long int n;
	cin>>n;
	while(n!=-1)
	{
		n=n-1;
		if(n%3!=0)
		{
			cout<<"N"<<endl;
			cin>>n;
			continue;
		}
		long long int y=n/3;
		long long int x=sqrt(y);
		if(x*(x+1)==y)
			cout<<"Y"<<endl;
		else
			cout<<"N"<<endl;
		cin>>n;
	}
	
}