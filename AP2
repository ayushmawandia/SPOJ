#include <iostream>

using namespace std;


int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		long long int a,b,s;
		cin>>a>>b>>s;
		long long int sum=a+b;
		long long int diff=b-a;
		int l=2*s/sum;
		int r=diff/(l-5);
		int x=a-3*r;
		cout<<l<<endl;
		for(int i=1;i<=l;i++)
		{
			cout<<x+i*r<<" ";
		}
		cout<<endl;
	}
	
	
} 