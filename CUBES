#include<cstring>
#include <iostream>
#include <unordered_map>
using namespace std;

int main()
{
   for(int i=2;i<=100;i++)
   {
       for(int j=2;j<=100;j++)
   {
        for(int k=j+1;k<=100;k++)
   {
        for(int l=k+1;l<=100;l++)
   {
        if(i*i*i==(j*j*j+k*k*k+l*l*l))

            cout<<"Cube = "<<i<<", Triple = ("<<j<<","<<k<<","<<l<<")"<<endl;
   }
   }

   }
   }
}
