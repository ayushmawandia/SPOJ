#include<iostream>
#include<cmath>
using namespace std;
int main()
{
	int t;
	cin>>t;
	for(int j=1;j<=t;j++)
	{
		int n;
		cin>>n;
		long long int x=0,y=0;
		for(int i=0;i<n;i++)
		{
			int a;
			cin>>a;
			y=y+a;
			if(y<x)
				x=y;
		}
		x=abs(x);
		x++;
		cout<<"Scenario #"<<j<<": "<<x<<endl;
	}
	
	
}